format IVG-3 requires:IMPD-1
bounds 0,0,120,60
fill black
pen miter-limit:4
context [
	fill opacity:0.5
	context [
		fill red
		RECT 0,0,40,60
	]
	context [
		fill green opacity:0.25
		RECT 40,0,40,60
	]
	context [
		pen blue width:10 opacity:0.3
		fill none
		RECT 80,0,40,60
	]
]
context [
	fill rgb(1,0,0) opacity:0.5
	ELLIPSE 20,30,10
]
context [
	fill rgb(0,1,0) opacity:0.25
	ELLIPSE 60,30,10
]

