FORMAT IVG-2 requires:IMPD-1
BOUNDS 0,0,800,800

prng_x = 1;
prng = call [ prng_x = {$prng_x * 279470273 % 4294967291}; return $0 = {$prng_x / 4294967291} ]

drawStuff = [
	for i from:0 to:1 [
		$prng x; $prng y; $prng w; $prng h; $prng r; $prng g; $prng b
		fill gradient:[radial 0.5,0.5,0.5 from:rgb($r,$g,$b,1.0) to:#FF202020] relative:yes
		s = rect {$x * 800},{$y * 800},{$w * 400},{$h * 400}
		for j from:0 to:100 [
			$s
			$s
			$s
			$s
		]
	]
]

$drawStuff
