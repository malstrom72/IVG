format IVG-3 requires:IMPD-1
bounds 0,0,220,120

// Verifies: "An empty PATH or one without drawable segments is a no-op."
// Insert a PATH with only a move-to (no segments). Then draw two visible squares.

WIPE white
fill none
pen black width:4

// Left square (visible)
PATH [
	move-to 30,30
	line-to 70,30,70,70,30,70
	close
]

// Empty PATH: only a move-to (no drawable segments). Make pen none here to avoid any stroke end-cap dot.
pen none
PATH [ move-to 100,60 ]

// Right square (visible)
pen black width:4
PATH [
	move-to 110,30
	line-to 150,30,150,70,110,70
	close
]

