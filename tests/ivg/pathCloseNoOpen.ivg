format IVG-3 requires:IMPD-1
bounds 0,0,220,120

// Verifies: "close has no effect if no sub-path is open."
// Draw two squares; an extra close between sub-paths must not change output.

WIPE white
fill none
pen black width:4

// First square with an extra stray close after it
PATH [
	move-to 30,30
	line-to 70,30,70,70,30,70
	close
	close	// no effect; no sub-path is open here
	move-to 110,30
	line-to 150,30,150,70,110,70
	close
]

