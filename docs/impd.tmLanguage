		<key>assignment</key>
		<dict>
			<key>patterns</key>
			<array>
				<dict>
					<key>name</key>
					<string>storage.impd</string>
					<key>match</key>
					<string>\b[a-zA-Z$_][a-zA-Z0-9$_]*(?=\s*=\s*)</string>
				</dict>
			</array>
		</dict>
		<key>label</key>
