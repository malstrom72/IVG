			<key>patterns</key>
			<array>
				<dict>
					<key>name</key>
					<string>entity.name.label.impd</string>
					<key>match</key>
					<string>\b[a-zA-Z$_][a-zA-Z0-9$_]*(?=\s*:)</string>
				</dict>
			</array>
		</dict>
		<key>variable</key>
		<dict>
			<key>patterns</key>
			<array>
				<dict>
					<key>name</key>
