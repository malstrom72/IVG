				</dict>
			</array>
		</dict>
		<key>constant</key>
		<dict>
			<key>patterns</key>
			<array>
				<dict>
					<key>name</key>
					<string>constant.language.impd</string>
					<key>match</key>
					<string>(?i)\b(yes|no)\b</string>
				</dict>
				<dict>
					<key>name</key>
					<string>constant.language.impd</string>
					<key>match</key>
					<string>(?i)\b(pi)\b</string>
				</dict>
			</array>
		</dict>
		<key>instruction</key>
		<dict>
			<key>patterns</key>
			<array>
				<dict>
					<key>begin</key>
					<string>(?i)\bformat\s*(\S*)\s*requires\s*:\s*(\S*)</string>
					<key>end</key>
					<string>(?=.)</string>
					<key>captures</key>
					<dict>
						<key>0</key>
						<dict>
							<key>name</key>
							<string>entity.name.function.impd</string>
						</dict>
						<key>1</key>
						<dict>
							<key>name</key>
							<string>string.other.impd</string>
						</dict>
						<key>2</key>
						<dict>
							<key>name</key>
							<string>string.other.impd</string>
						</dict>
					</dict>
				</dict>
				<dict>
					<key>name</key>
					<string>entity.name.function.impd</string>
					<key>match</key>
					<string>(?i)\b(format|meta|debug|trace|stop|call|include|local|return|if|repeat|for)\b</string>
				</dict>
			</array>
		</dict>
		<key>number</key>
		<dict>
			<key>patterns</key>
			<array>
				<dict>
					<key>name</key>
					<string>constant.numeric.hex.impd</string>
					<key>match</key>
					<string>(?&lt;![a-zA-Z0-9])0x[0-9A-Fa-f]+</string>
				</dict>
				<dict>
					<key>name</key>
					<string>constant.numeric.impd</string>
					<key>match</key>
					<string>(?&lt;![a-zA-Z0-9])[-+]?[0-9]*\.?[0-9]+([eE][-+]?[0-9]+)?(\s*%)?</string>
				</dict>
			</array>
		</dict>
		<key>function</key>
		<dict>
			<key>patterns</key>
			<array>
				<dict>
					<key>name</key>
					<string>support.function.math.impd</string>
					<key>match</key>
					<string>(?i)\b(abs|acos|asin|atan|ceil|cos|cosh|exp|floor|log|log10|sin|sinh|sqrt|tan|tanh)(?=\()</string>
				</dict>
				<dict>
					<key>name</key>
					<string>support.function.other.impd</string>
					<key>match</key>
					<string>(?i)\b(len)(?=\()</string>
				</dict>
			</array>
